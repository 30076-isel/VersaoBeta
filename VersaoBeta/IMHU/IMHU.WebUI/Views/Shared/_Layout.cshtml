<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <link href="~/Content/bootstrap-3.3.4-dist/css/bootstrap.min.css" rel="stylesheet" />

        @Styles.Render("~/Content/bootstrap-3.3.4-dist/css")
        @Styles.Render("~/Content/styleLogo.css")
        @Scripts.Render("~/bundles/modernizr")
    </head>

<body style="background-image: url(../Images/mochaGrunge.png)";>

   
    <div class="wrap clearfix" style="margin-left: 95px;margin-left: 95px;height: 100px;">
      
        <h1 class="logo imhu-logo" style="margin-top: 0px;position: relative;height: 100%;">
            <a href="/" title="Inicio">
                <img src="~/Images/logo.png" style="position: relative;height: 100%;"/></a>
        </h1>
    </div>


    <div class="navbar-wrapper">
        <div class="container">
            <nav class="navbar navbar-inverse navbar-fixed-top" style="margin-top: 100px;">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Inventory Management in Health Units</a>
                    </div>
                </div>
            </nav>
        </div>
    </div>
 


    <div class="container">
        @RenderBody()
    </div>


    <footer style="margin-left: 110px;">
   
	    <div class="copy">Copyright @DateTime.Now.Year.ToString() IMHU. Todos os Direitos Reservados</div>
       
    </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="~/Content/bootstrap-3.3.4-dist/js/bootstrap.min.js"></script>
    <script src="~/Content/bootstrap-3.3.4-dist/js/bootstrap.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
